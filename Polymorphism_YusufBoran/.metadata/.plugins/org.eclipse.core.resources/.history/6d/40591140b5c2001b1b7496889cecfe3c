package Ornek1.classes;

import Ornek1.interfaces.IkiBoyutlu;
import Ornek1.interfaces.UcBoyutlu;

public class Silindir implements IkiBoyutlu, UcBoyutlu {

	private int yaricap, yukseklik;

	public Silindir(int yaricap, int yukseklik) {
		this.yaricap = yaricap;
		this.yukseklik = yukseklik;
	}

	@Override
	public void hacimHesapla() {
		tabanAlani();
	}

	@Override
	public void alanHesapla() {

	}

	private double tabanAlani() {
		return yaricap*yaricap*Math.PI;
	}

}
